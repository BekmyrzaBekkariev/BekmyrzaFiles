import pyowm

owm = pyowm.OWM('65e7d294c9173803b68c2148b1f8b0db', Language = "ru") 

place = input("Ведите в каком в городе/стране?:")

observation = owm.weather_at_place(place)
w = observation.get_weather()

temp = w.get_temperature("celsius")["temp"]

print("В городе" + place + "сейчас" + w.get_datailed_status())
print("Температура сейчас в раёне" + str(temp))

if temp < 10:
	print("сейчас холодно, одевайся как танк!")
elif temp <20:
	print("сейчас холодно, оденься по теплее")
else:
	print("Температура норм, одевайся как угодно.")

print(w)